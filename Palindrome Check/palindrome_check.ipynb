    "#This function takes a non-empty string and that returns a boolean\n",
    "#representing whether the string is a palindrome\n",
    "\n",
    "def isPalindrome(string):\n",
    "    newString=\"\"\n",
    "    \n",
    "    for i in reversed(range(len(string))):\n",
    "        newString += string[i]\n",
    "    return newString == string"
   ]
  }
